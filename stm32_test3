	  if(USART1_rx_end){
	  if(strcmp((const char*)Rx_Buffer,"LED2 ON")==0||strcmp((const char*)Rx_Buffer,"LED2 ON ON")==0||strcmp((const char*)Rx_Buffer,"LED2 ON OFF")==0||strcmp((const char*)Rx_Buffer,"LED2 ON ONF")==0){
		  HAL_GPIO_WritePin(GPIOE, led2_Pin|led3_Pin, GPIO_PIN_RESET);
	      HAL_GPIO_WritePin(GPIOE, led1_Pin, GPIO_PIN_SET);
	      printf("ACK,LED2 ON\r\n");
	  	  }
	  else if(strcmp((const char*)Rx_Buffer,"LED3 ON")==0||strcmp((const char*)Rx_Buffer,"LED3 ON ON")==0||strcmp((const char*)Rx_Buffer,"LED3 ON OFF")==0||strcmp((const char*)Rx_Buffer,"LED3 ON ONF")==0){
	  		 HAL_GPIO_WritePin(GPIOE, led1_Pin|led3_Pin, GPIO_PIN_RESET);
	           HAL_GPIO_WritePin(GPIOE, led2_Pin, GPIO_PIN_SET);
	           printf("ACK,LED3 ON\r\n");
	  	  }
	  else if(strcmp((const char*)Rx_Buffer,"LED4 ON")==0||strcmp((const char*)Rx_Buffer,"LED4 ON ON")==0||strcmp((const char*)Rx_Buffer,"LED4 ON OFF")==0||strcmp((const char*)Rx_Buffer,"LED4 ON ONF")==0){
	  		  HAL_GPIO_WritePin(GPIOE, led1_Pin|led2_Pin, GPIO_PIN_RESET);
	  		  HAL_GPIO_WritePin(GPIOE, led3_Pin, GPIO_PIN_SET);
	  		 printf("ACK,LED4 ON\r\n");
	  	  }
	  else if(strcmp((const char*)Rx_Buffer,"ALL LED ON")==0||strcmp((const char*)Rx_Buffer,"ALL LED ONF")==0){
	  		  HAL_GPIO_WritePin(GPIOE, led1_Pin|led2_Pin|led3_Pin, GPIO_PIN_SET);
	  		 printf("ACK,ALL ON\r\n");
	  	  }
	  else if(strcmp((const char*)Rx_Buffer,"ALL LED OFF")==0){
	  		  HAL_GPIO_WritePin(GPIOE, led1_Pin|led2_Pin|led3_Pin, GPIO_PIN_RESET);
	  		 printf("ACK,ALL OFF\r\n");
	  	  }
	  	  USART1_len=0;
          USART1_rx_end=0;
	  }
