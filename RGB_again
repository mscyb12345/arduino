#include <LiquidCrystal.h>

#define RS  11
#define RW  12
#define E   13
#define D0  2
#define D1  3
#define D2  4
#define D3  5
#define D4  6
#define D5  7
#define D6  8
#define D7  9

LiquidCrystal LCD(RS, RW, E, D0, D1, D2, D3, D4, D5, D6, D7);

void setup() {
  LCD.begin(16, 2);
  LCD.clear();
}

void loop() {
  //첫째 줄 : DSM
  //둘째 줄 : 학번+이름
  //출력하는 코드 작성
}
