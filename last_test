void setup() {
  pinMode(A0,OUTPUT);
  pinMode(A1,OUTPUT);
  pinMode(A2,OUTPUT);
}

void loop() {
  analogWrite(A0,198);
  analogWrite(A1,170);
  analogWrite(A2,0);
  delay(500);
    analogWrite(A0,200);
  analogWrite(A1,0);
  analogWrite(A2,200);
  delay(500);
     analogWrite(A0,0);
  analogWrite(A1,200);
  analogWrite(A2,200);
  delay(500);
     analogWrite(A0,250);
  analogWrite(A1,110);
  analogWrite(A2,100);
  delay(500);
    analogWrite(A0,19);
  analogWrite(A1,70);
  analogWrite(A2,220);
  delay(500);
     analogWrite(A0,200);
  analogWrite(A1,150);
  analogWrite(A2,50);
  delay(500);
     analogWrite(A0,198);
  analogWrite(A1,70);
  analogWrite(A2,150);
  delay(500);
     analogWrite(A0,0);
  analogWrite(A1,250);
  analogWrite(A2,0);
  delay(500);
     analogWrite(A0,255);
  analogWrite(A1,255);
  analogWrite(A2,255);
  delay(500);
   
}
